msgstr ""

#. STRINGS.NPC.APOTHECARY.QUESTS.twn_shop_apothecary.attract_resident.TALK3
msgctxt "STRINGS.NPC.APOTHECARY.QUESTS.twn_shop_apothecary.attract_resident.TALK3"
