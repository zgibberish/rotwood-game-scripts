msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT2B_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT2B_RESPONSE"
msgid "					player:\n						Like maybe before I get knocked out?\n					agent:\n						!shrug\n						Hey, pobody's nerfect.\n				"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT_1A
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT_1A"
msgid "Did... did I die?"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT_1B
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT_1B"
