msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT2B_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT2B_RESPONSE"
msgid "						agent:\n							!shocked\n							Hunter!\n							!angry\n							Don't speak like that.\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT3_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT3_RESPONSE"
msgid "						agent:\n							!shocked\n							Woah! In all the commotion I totally forgot!\n							!point\n							That right there is a <#KONJUR>Heartstone</>, my friend.\n							!agree\n							It's <i>very</i> important.\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT4A_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT4A_RESPONSE"
msgid "						agent:\n							!angry\n							No!\n							!think\n							Well, sort of.\n							!angry\n							But no!\n							!shrug\n							I'll just show you.\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT4B_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT4B_RESPONSE"
msgid "						agent:\n							!agree\n							That's precisely what I want to show you.\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT5A_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT5A_RESPONSE"
msgid "						agent:\n							!think\n							Oh, sorry.\n							!point\n							The <#KONJUR>Wellspring</> is that big purple rock here in town.\n							!shrug\n							Y'know, near where I dropped you off.\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_1A
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_1A"
msgid "Thanks! I'm feeling good about it."
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_1B
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_1B"
msgid "I was screaming inside the entire time!"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_1C
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_1C"
msgid "It was literally so easy."
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_2A
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_2A"
msgid "We still haven't found Berna or Hamish."
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_2B
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_2B"
msgid "D'you think our Armourer and Blacksmith are goners yet?"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_3
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_3"
msgid "Do you know anything about this rock the Mother Treek dropped?"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_4A
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_4A"
msgid "Can I eat it?"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_4B
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_4B"
msgid "What's it do?"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_5A
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_5A"
msgid "Wellspring? What's that?"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_5B
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_5B"
msgid "Sure thing, Flitt."
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_5B_ALT
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT_5B_ALT"
msgid "Ohhh, the Wellspring. Sure thing, Flitt."
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.TALK
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.TALK"
msgid "						agent:\n							!shocked\n							Holy moly, that was incredible to watch!\n							!point\n							I can't believe you downed a <#RED>Mother Treek</> on your very first Hunt!\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.TALK2
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.TALK2"
msgid "						agent:\n							!thinking\n							I think you might become a Hunter to rival <#BLUE>Toot</> in his heyday.\n							!clap\n							Oh! Speaking of which, <#BLUE>Toot</> made it back to camp while we were out!\n							!gesture\n							You should go introduce yourself if you haven't had a chance yet.\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.TALK3
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.TALK3"
msgid "						agent:\n							!gesture\n							While you were hunting I spotted a section of the forest where the treeline had been disturbed, though.\n							!agree\n							It's possible one of our people landed there.\n							!dubious\n							I think we should keep searching the <#RED>Great Rotwood Forest</>, too.\n							!gesture\n							Both locations available on your map. I'll leave it up to you where we go next.\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.TALK4
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.TALK4"
msgid "						agent:\n							!gesture\n							I'd prefer not to touch it. Do me a favour, won't you?\n							!point\n							Place <#RED>Mother Treek's</> <#KONJUR>Heartstone</> in the <#RED>Wellspring</>.\n					"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_regular.END
