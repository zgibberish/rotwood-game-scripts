msgctxt "STRINGS.NPC.POTIONMAKER.QUESTS.first_meeting.TALK"
msgid "				agent:\n					!shocked\n					Goodness <i>graciousss!</i> Is that a cussstomer I see?\n					!greet\n					Greetings and sssalutations, my fine <#BLUE>{species}</> friend! Welcome to the <#RED>Rotwood</>!\n					!point\n					<i>Boy howdy</i>, have I ever got a business proposition for YOU!\n			"
msgstr ""

#. STRINGS.NPC.POTIONMAKER.QUESTS.first_meeting.TALK2
msgctxt "STRINGS.NPC.POTIONMAKER.QUESTS.first_meeting.TALK2"
msgid "				agent:\n					!agree\n					One sip with <p bind='Controls.Digital.USE_POTION' color=BTNICON_DARK>'s guaranteed to <#RED>Heal</> your woundsss, clear your skin, and leave your breath minty fresh for that <i>ssspecial someone</i> back home.\n					!shocked\n					[recipe:admission_recipe] And all for only a modessst sum of <#KONJUR>Teffra</>!\n			"
msgstr ""

#. STRINGS.NPC.POTIONMAKER.QUESTS.first_meeting.buffer.TALK
