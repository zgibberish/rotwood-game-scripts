msgid "				agent:\n					!wavelunn\n					My leather cutter! He's my best pal.\n				player:\n					Your best pal is your leatherworking tool? That's--\n				agent:\n					!angry\n					--That's <i>what</i>?\n			"
msgstr ""

#. STRINGS.NPC.ARMORSMITH.QUESTS.tertiary_twn_meeting_armorsmith.OPT4B_RESPONSE
msgctxt "STRINGS.NPC.ARMORSMITH.QUESTS.tertiary_twn_meeting_armorsmith.OPT4B_RESPONSE"
msgid "				player:\n					--You want me to fight <#RED>Rots</> unprotected, so you can upgrade my armour to protect me while fighting <#RED>Rots</>?\n				agent:\n					!shrug\n					Yeah!\n					!gesture\n					We need the <#RED>Rot Materials</> to upgrade the armour, silly.\n					!laugh\n					And as Armourers say back home... no cuts, no glory!\n					!gesture\n					You'll be fine! Probably.\n			"
msgstr ""

#. STRINGS.NPC.ARMORSMITH.QUESTS.tertiary_twn_meeting_armorsmith.OPT4C_RESPONSE
msgctxt "STRINGS.NPC.ARMORSMITH.QUESTS.tertiary_twn_meeting_armorsmith.OPT4C_RESPONSE"
