#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT1B_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT1B_RESPONSE"
msgid "					agent:\n						!point\n						See? I knew I liked you. Always looking for silver linings.\n						!dubious\n						What <i>is</i> being unconscious in a deadly forest if not a little \"nap\"?\n				"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT1C_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT1C_RESPONSE"
msgid "					agent:\n						!dubious\n						Easy now, we're out of the woods. Literally and figuratively.\n						!point\n						Keep that fighting spirit though, that's good stuff.\n				"
msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT2A_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.pre_miniboss_death_convo.OPT2A_RESPONSE"
