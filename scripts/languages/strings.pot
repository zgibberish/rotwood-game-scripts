#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.RELOADING_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.RELOADING_RESPONSE"
msgid "					agent:\n						!think\n						Reloading can be fiddly if you're new to this puppy.\n						!gesture\n						Hit <p bind='Controls.Digital.DODGE' color=BTNICON_DARK> to start a reload, then hit <p bind='Controls.Digital.ATTACK_LIGHT' color=BTNICON_DARK> just as your <#RED>Cannon</> touches the ground to finish'r off.\n						!point\n						You can practice on the <#RED>Training Dummies</> while you're in camp if you need to.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.RELOADING_RESPONSE_ALT
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.RELOADING_RESPONSE_ALT"
msgid "					agent:\n						!angry\n						Okay. <#KONJUR><i><z 0.8>(cough cough)</z></i></> But LISTEN UP now.\n						!agree\n						Just hit <p bind='Controls.Digital.DODGE' color=BTNICON_DARK>, then hit <p bind='Controls.Digital.ATTACK_LIGHT' color=BTNICON_DARK> as your <#RED>Cannon</> touches the ground to reload.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.SKIPINFO_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.SKIPINFO_RESPONSE"
msgid "					agent:\n						!shrug\n						Just you don't blow yourself sky high and I'm happy. Anyway--\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.STARTEXPLAINER_RESPONSE
