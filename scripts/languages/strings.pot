#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.TALK2
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.TALK2"
msgid "					agent:\n						!point\n						Good Hunter instinct on ya.\n						!gesture\n						'Ere. Take this.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.TALK_ALLDONE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.TALK_ALLDONE"
msgid "					agent:\n						!gesture\n						Play around, get a knack for it.\n						!shrug\n						You'll know you've learned well if the <#RED>Rots</> are dying, but you aren't.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.TALK_GIVE_WEAPON
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_cannon.TALK_GIVE_WEAPON"
msgid "					agent:\n						!agree\n						Mhm. An old one mind you, but old things can still pack a punch.\n						!point\n						Dug it out of some pretty messed up cargo crates.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT1A_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT1A_RESPONSE"
msgid "						agent:\n							!laugh\n							HA-HA! <#KONJUR><i><z 0.8>(cough cough)</z></i></> <#KONJUR><i><z 0.8>(cough cough)</z></i></> <#KONJUR><i><z 0.8>(cough cough)</z></i></>\n							!dubious\n							And forsake my sweetie girlie, bow 'n' arrow?\n							!disagree\n							Not a chance.\n					"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT1B_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT1B_RESPONSE"
msgid "						agent:\n							!gesture\n							What, never fought with a <#RED>Spear</> before?\n							!think\n							The most crucial lesson is this: To do a <#BLUE>Focus Hit</> with a <#RED>Spear</>, strike an <#RED>Enemy</> with the very tip.\n							!point\n							You can practice the correct distancing on our <#RED>Training Dummies</> if you need.\n							!shrug\n							And if you and the <#RED>Spear</> don't get along, your <#RED>Hammer</> will be waiting dutifully in your inventory. <#KONJUR><i><z 0.8>(cough cough)</z></i></>\n							!gesture\n							Find the weapons that make your soul sing. They're all that stand between a Hunter and death.\n					"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT1C_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT1C_RESPONSE"
msgid "						agent:\n							!gesture\n							Hey, one more thing. You can't swap gear once Flitt takes off.\n							!agree\n							Keep that in mind when you head out.\n					"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT_1A
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT_1A"
msgid "Don't you want it?"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT_1A_ALT
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT_1A_ALT"
msgid "Don't you want the Spear?"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT_1B
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT_1B"
msgid "How do I use it?"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT_1C
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.OPT_1C"
msgid "Thanks, Toot! I'll try it out!"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.TALK_GIVE_WEAPON
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_polearm.TALK_GIVE_WEAPON"
msgid "						agent:\n							!greet\n							Hunter! <#KONJUR><i><z 0.8>(cough cough)</z></i></> Come 'ere.\n							!agree\n							Good old Flitt recovered a cargo crate with that flying ma-jigger of theirs while you were out. \n							!point\n							Just cracked 'er open and it looks like the Foxtails have <#RED>Spears</> again!\n							!gesture\n							Here. Try this puppy on for size.\n					"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.BYE_A
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.BYE_A"
msgid "Ayy!"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.BYE_B
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.BYE_B"
msgid "<i>Toot</i>-a-loo! Hehe."
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1A_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1A_RESPONSE"
msgid "					agent:\n						!point\n						A <#RED>Striker</>, actually.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1B_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1B_RESPONSE"
msgid "					agent:\n						!think\n						Well when you're up close and personal with a <#RED>Rot</>, you can do a nice reliable punch with <p bind='Controls.Digital.ATTACK_LIGHT' color=BTNICON_DARK>.\n						!point\n						Chuck a <#RED>Striker</> by hitting <p bind='Controls.Digital.ATTACK_HEAVY' color=BTNICON_DARK>. You've got <#RED>2 Strikers</> total.\n						!bliss\n						You can do some real awesome moves with these. Like punch'em clean out of the air and into a <#RED>Rot</>'s face!\n						!point\n						Time your throws and punches to keep a <#RED>Striker</> airborne, and you've got a one-way ticket to <#BLUE>Focus Hit</>-city.\n						!disagree\n						Once the <#RED>Striker</i> touches the ground though, your <#BLUE>Focus Hit</> streak'll end.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1B_RESPONSE_ALT
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1B_RESPONSE_ALT"
msgid "					player:\n						I wasn't listening the first time.\n					agent:\n						!angry\n						Well, LISTEN UP this time!\n						!gesture\n						'Kay now. Hit <p bind='Controls.Digital.ATTACK_LIGHT' color=BTNICON_DARK> and you'll do a good ol' close quarters punch.\n						!point\n						Hit <p bind='Controls.Digital.ATTACK_HEAVY' color=BTNICON_DARK> to throw one of your <#RED>2 Strikers</>.\n						!agree\n						Keepin' the <#RED>Strikers</> airborne will rack up the <#BLUE>Focus Hits</>, but if one hits the ground, your streak's kaput.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1C_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1C_RESPONSE"
msgid "					agent:\n						!gruffnod\n						<#KONJUR><i><z 0.8>(cough cough)</z></i></> Knock'm dead, Hunter.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1C_RESPONSE_ALT
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT1C_RESPONSE_ALT"
msgid "					agent:\n						!eyeroll\n						Go, uh... <#KONJUR><i><z 0.8>(cough cough)</z></i></> <i>have a ball</i> out there.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT2A_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT2A_RESPONSE"
msgid "					agent:\n						!angry\n						<#KONJUR><i><z 0.8>(cough cough)</z></i></> <#KONJUR><i><z 0.8>(cough cough)</z></i></> <#KONJUR><i><z 0.8>(cough cough)</z></i></> It's a <#RED>Striker</>!\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT2B_RESPONSE
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT2B_RESPONSE"
msgid "					agent:\n						!think\n						...<#KONJUR><i><z 0.8>(cough cough)</z></i></>\n						!shrug\n						I could end you.\n				"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1A
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1A"
msgid "Wow! It's... a ball?"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1B
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1B"
msgid "How do I even use this?"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1B_ALT
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1B_ALT"
msgid "Can you tell me how to use it again?"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1C
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1C"
msgid "Thanks, Toot!"
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1C_ALT
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_1C_ALT"
msgid "Seriously though, thanks Toot."
msgstr ""

#. STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_2A
msgctxt "STRINGS.NPC.DOJO_MASTER.QUESTS.WEAPON_UNLOCKS.twn_unlock_shotput.OPT_2A"
