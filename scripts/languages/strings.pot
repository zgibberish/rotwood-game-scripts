msgstr ""

#. STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT2A_RESPONSE
msgctxt "STRINGS.NPC.SCOUT.QUESTS.main_defeat_megatreemon.celebrate_defeat_boss.defeated_first_run.OPT2A_RESPONSE"
