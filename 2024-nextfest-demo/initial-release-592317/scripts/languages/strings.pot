msgstr ""

#: Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_2C
msgctxt "Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_2C"
msgid "It's gonna be okay, Flitt. Who's missing?"
msgstr ""

#: Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_2D
msgctxt "Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_2D"
