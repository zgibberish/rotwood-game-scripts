msgstr ""

#: Convo.twn_missingfriends_scout.CHAT_TOWN4.hamishs_book.giver.STATE_0.TALK
msgctxt "Convo.twn_missingfriends_scout.CHAT_TOWN4.hamishs_book.giver.STATE_0.TALK"
msgid "agent:\n								!dubious\n								I found the novel <#BLUE>Hamish's</> was reading.\n								!disagree\n								Poor guy. He doesn't sleep well if he doesn't read before bed."
msgstr ""

#: Convo.twn_missingfriends_scout.CHAT_TOWN5.who_am_i_looking_for.giver.STATE_0.OPT1A_RESPONSE
msgctxt "Convo.twn_missingfriends_scout.CHAT_TOWN5.who_am_i_looking_for.giver.STATE_0.OPT1A_RESPONSE"
msgid "agent:\n								!gesture\n								Oh, <#BLUE>Berna</> and <#BLUE>Hamish</>?"
msgstr ""

#: Convo.twn_missingfriends_scout.CHAT_TOWN5.who_am_i_looking_for.giver.STATE_0.OPT1B_RESPONSE
msgctxt "Convo.twn_missingfriends_scout.CHAT_TOWN5.who_am_i_looking_for.giver.STATE_0.OPT1B_RESPONSE"
msgid "agent:\n								!clap\n								The Damselfly awaits!"
msgstr ""

