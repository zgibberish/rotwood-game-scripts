msgstr ""

#: Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_3A
msgctxt "Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_3A"
msgid "What are you gonna do?"
msgstr ""

#: Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_3B
msgctxt "Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_3B"
msgid "But I've never been in a real fight before!\n<i><#RED><z 0.7>(Explain controls)</></i></z>"
msgstr ""

#: Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_3C
msgctxt "Convo.main_defeat_megatreemon.CHAT_DUNGEON0.quest_intro.giver.STATE_0.OPT_3C"
